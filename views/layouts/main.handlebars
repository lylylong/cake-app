          <a class="nav-link" href="/dashboard">Order</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/gallery">Gallery</a>
        </li>
        <li class="nav-item">
          <a id="logout" class="nav-link" href="/">Logout</a>
        </li>
        {{else}}
        <a class="nav-link" href="/login">Login/Order</a>
        <li class="nav-item">
          <a class="nav-link" href="/gallery">Gallery</a>
        </li>
        {{/if}}

      </ul>
    </div>
  </nav>

